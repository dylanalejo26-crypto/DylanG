import random

n = int(input("Ingrese la cantidad de elementos: "))

arreglo = []
num = random.randint(0, 9) 
arreglo.append(num)

for i in range(1, n):
    limite_superior = ((arreglo[i-1] // 10) + 1) * 10 - 1
    if arreglo[i-1] == limite_superior:
        
        num = limite_superior + 1
    else:
        num = random.randint(arreglo[i-1] + 1, limite_superior)
    arreglo.append(num)

print("Arreglo generado:", arreglo)
